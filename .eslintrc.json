{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
